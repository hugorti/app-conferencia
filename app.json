{
  "expo": {
    "name": "Labotrat Conf",
    "slug": "labotrat-conf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/caixa.png",
    "scheme": "Labotrat Conf.",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/caixa.png",
        "backgroundColor": "#865BA6"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.taskseat.labotratconf"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/caixa.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/caixa.png",
          "resizeMode": "contain",
          "backgroundColor": "#865BA6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "56bcd44d-d22a-468b-8b6a-8dbbca7cf289"
      }
    }
  }
}
