{
  "expo": {
    "name": "Labotrat Conf.",
    "slug": "Labotrat Conf.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/caixa.png",
    "scheme": "Labotrat Conf.",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/caixa.png",
        "backgroundColor": "#865BA6"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/caixa.png"
    },
    "plugins": [
  "expo-router",
  [
    "expo-splash-screen",
    {
      "image": "./assets/images/caixa.png",
      "resizeMode": "contain",
      "backgroundColor": "#865BA6"
    }
  ]
],

    "experiments": {
      "typedRoutes": true
    }
  }
}
